package beanSession;

public class User {

    private long   ID;
    private String email;
    private String pwd;

    private Set<int> listIdItems;

    public void setEmail( String email ) {
        this.email = email;
    }

    public String getPwd() {
        return pwd;
    }

    public void setPwd( String pwd ) {
        this.pwd = pwd;
    }

    public String getEmail() {
        return email;
    }

    public long getID() {
        return ID;
    }

    public void setID( long iD ) {
        ID = iD;
    }

    public void setPWD( String motDePasse ) {
        this.pwd = pwd;
    }

    public String getPWD() {
        return pwd;
    }

}
